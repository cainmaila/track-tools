{
  "name": "track-tools",
  "version": "0.1.8",
  "private": true,
  "scripts": {
    "app": "node app/app.js",
    "dev": "vue-cli-service serve",
    "build": "cross-env BUILD=yes vue-cli-service build",
    "start": "cross-env NODE_ENV=production node app/app.js",
    "lint": "vue-cli-service lint",
    "sdk-editor": "rollup -c rollup.editor.config.js",
    "sdk-player": "rollup -c rollup.player.config.js",
    "sdk": "yarn sdk-editor && yarn sdk-player",
    "i18n-sync": "sync-i18n --files **/src/locale/*.json --primary en --createResources zh-TW"
  },
  "dependencies": {
    "@pixi/graphics-extras": "^6.1.2",
    "@pixi/math-extras": "^6.1.2",
    "@vueuse/core": "^6.4.1",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "pixi-viewport": "^4.32.0",
    "pixi.js": "^6.1.2",
    "vue": "^3.0.0",
    "vue-i18n": "^9.1.7"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "i18next-json-sync": "^2.3.1",
    "postcss-apply": "^0.12.0",
    "postcss-css-variables": "0.14.0",
    "postcss-custom-media": "7.0.8",
    "postcss-define-property": "^0.5.0",
    "postcss-import": "12",
    "postcss-preset-env": "^6.7.0",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}

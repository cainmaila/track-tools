#app {
  font-family: Avenir, Helvetica, Arial, sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  height: 100%;
  overflow: hidden;
}
.page {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.rest {
  position: relative;
}
.hight_100 {
  height: 100%;
}
.flex-center {
  display: flex;
  justify-content: center;
  align-items: center;
}
.flex-between {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.flex {
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.txt-hidden {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.txt-enter {
  word-wrap: break-word;
}
